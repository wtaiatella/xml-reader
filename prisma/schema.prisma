generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model XmlConfig {
  key          String @default(cuid())  @id
  Width       	 Int
  Height       	 Int
  newSizeX       Int?   
  newSizeY       Int?
  limitLeft      Int?
  limitRight     Int?
  hasRightMenu   Boolean @default(false)
  
}


